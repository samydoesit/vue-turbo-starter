{
  "name": "nuxt-app",
  "version": "0.0.0",
  "scripts": {
    "test:unit": "vitest --environment jsdom",
    "prepare": "dotenv -e ../../.env -- nuxi prepare",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 dotenv -e ../../.env -- nuxi dev",
    "build": "dotenv -e ../../.env -- nuxi build",
    "start": "dotenv -e ../../.env -- node .output/server/index.mjs",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,md,mdx --ignore-path .gitignore",
    "typecheck": "dotenv -e ../../.env -- nuxi typecheck",
    "test:e2e": "dotenv -e ../../.env -- playwright test --project=chromium",
    "test:e2e:ui": "dotenv -e ../../.env -- playwright test --ui"
  },
  "devDependencies": {
    "@nuxt/devtools": "1.0.0",
    "@pinia/nuxt": "0.5.1",
    "@playwright/test": "1.39.0",
    "@unocss/nuxt": "0.56.5",
    "@vitest/ui": "0.34.6",
    "@vue/test-utils": "2.4.1",
    "css-preset": "workspace:*",
    "nuxt": "3.8.0",
    "pinia-store": "workspace:*",
    "pinia": "2.1.7",
    "sass-loader": "13.3.2",
    "sass": "1.69.5",
    "tsconfig": "workspace:*",
    "typescript": "5.2.2",
    "ui": "workspace:*",
    "vite": "4.5.0",
    "vitest": "0.34.6",
    "vue-toastification": "2.0.0-rc.5",
    "vue": "3.3.7",
    "webpack": "5.89.0"
  }
}
